<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.Mappers.OrderMapper">
	<select id="selectTest" resultType="com.example.demo.DTO.OrderItemDTO">
		SELECT * FROM EMP 
	</select>
	<select id="selectTest2" resultType="com.example.demo.DTO.TestVo">
		SELECT * FROM EMP 
	</select>
	<insert id="createTest" parameterType="com.example.demo.DTO.TestVo">
		INSERT INTO EMP(NUM,NAME) VALUES(#{id},#{name})
	</insert>
	<insert id="insert" parameterType="com.example.demo.DTO.TestVo">
		INSERT INTO EMP(NUM,NAME) VALUES(#{id},#{name})
	</insert>
	
	<delete id="delete" >
		DELETE EMP WHERE NUM = #{id}
	</delete>
	
	<update id = "update">
        UPDATE     EMP
        SET     NUM = #{id}
                , NAME = #{name}
        WEHRE     NUM = #{id}  
    </update>

</mapper>

